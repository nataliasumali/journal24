<h1>Log In</h1>

<%= form_tag sessions_path do %>

<div class="field">
	<%= label_tag :email %><br/>
	<%= text_field_tag :email, params[:email] %>
</div>

<br/>

<div class="field">
	<%= label_tag :password %><br/>
	<%= password_field_tag :password, params[:password] %>
</div>

<br/>

<div class="actions">
	<%= submit_tag "Log_In" %>
</div>

<% end %>